<h2>{{name}}</h2>
{{#paper-content classNames="md-whiteframe-z1"}}
    {{#paper-list}}
      {{#each model}}
        {{#paper-item}}
          {{#paper-tile-content}}
            <div class="expense-detail" {{action 'edit' this}}>
              <h3>{{category}}</h3>
              <h4>{{date}}</h4>
            </div>
            <div class="expense-delete" {{action 'delete' this}}>
              <md-icon id="ember785" class="ember-view paper-icon ic-delete"></md-icon>
            </div>
            <div class="expense-amount">
              <h3>R {{amount}}</h3>
            </div>
          {{/paper-tile-content}}
        {{/paper-item}}
        {{paper-divider}}
      {{/each}}
    {{/paper-list}}
  {{/paper-content}}